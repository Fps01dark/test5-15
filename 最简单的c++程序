}
#endif
int main()
{
	int a = 0;
	int b = 0;
	int sum = 0;
	cin >> a >> b;
	sum = a + b;
	cout << "a="<<a<<" b="<<b<<'\n'<<"a+b=" << sum << endl;
	return 0;

}
class Student
{
private:
	int num;
	int score;
public:
	void setdata()
	{
		cin >> num;
		cin >> score;
	}
	void dispaly()
	{
		cout << "num=" << num << endl;
		cout << "score=" << score << endl;
	};
};
Student stud1, stud2;
int main()
{
	stud1.setdata();
	stud2.setdata();
	stud1.dispaly();
	stud2.dispaly();
	return 0;
}
