void choose(int arr[])
{
	int i = 0;
	int j = 0;
	for (i = 0; i < 4-1; i++)//第四步没必要进行
	{
		int min = i;
		for (j = i; j <4; j++)
			if (arr[min] > arr[j])
				min = j;
		int tmp = arr[min];
		arr[min] = arr[i];
		arr[i] = tmp;
	}
}

int main()
{
	int arr[4] = { 0 };
	int i = 0;
	for (i = 0; i < 4; i++)
	{
		scanf("%d", &arr[i]);
	}
	choose(arr);//不能arr[4]
	for (i = 0; i < 4; i++)
	{
		printf(" %d ", arr[i]);
	}
	return 0;
}
